!function(){"use strict";var inputs=document.querySelectorAll("input.numeric");Array.prototype.forEach.call(inputs,function(element){element.addEventListener("keyup",function(){if(/\D/g.test(element.value)){element.value=element.value.replace(/\D/g,"")}})});var heroImages=document.getElementsByClassName("js-hero-image");Array.prototype.forEach.call(heroImages,function(heroImage){if(heroImage.classList.contains("mti-hero-image--cover")&&!window.adobestock_heroImage.supportsObjectFit(heroImage)){window.adobestock_heroImage.objectFitFallback(heroImage)}})}();